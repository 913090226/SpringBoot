<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinjiang.mapper.IEmployeeMapper">
	<resultMap type="com.jinjiang.model.Employee" id="employeeResult">
		<result column="empno" property="empNo"/>
		<result column="name" property="name"/>
		<result column="hireday" property="hireday"/>
		<result column="salary" property="salary"/>
		<result column="deptNo" property="deptNo"/>
	</resultMap>
	<select id="findNameByempNo" parameterType="String" resultType="String">
		SELECT name FROM employee where empno = #{empNo}
	</select>
	<select id="findAll" resultMap="employeeResult">
		SELECT empno, name, hireday, salary, deptNo FROM employee
	</select>	
</mapper>